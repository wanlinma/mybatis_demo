<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.asiainfo.mybatis_demo.mapper.IdCardMapper">

    <insert id="insertIDcard" parameterType="com.asiainfo.mybatis_demo.entity.IdCard"
            useGeneratedKeys="true"
            keyProperty="idCardId"
            keyColumn="id_card_id"
    >
        insert into tb_id_card(id_card_number) values(#{idCard.idCardNumber})
    </insert>
</mapper>